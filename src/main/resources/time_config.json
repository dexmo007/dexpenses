{
  "Uhrzeit:?\\s*(\\d{2}:\\d{2}:\\d{2})": {
    "format": "HH:mm:ss"
  },
  "Uhrzeit:?\\s*(\\d{2})[\\s:;.,](\\d{2})": {
    "hour": 1,
    "minute": 2
  },
  "(\\d{2})[\\s;:.,](\\d{2})[\\s:;.,](\\d{2})": {
    "hour": 1,
    "minute": 2,
    "seconds": 3
  },
  "(\\d{2})[:;](\\d{2})": {
    "hour": 1,
    "minute": 2
  }
}